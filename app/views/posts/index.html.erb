<div class="container">
<header>
   <h1>POSTS</h1>
</header>
<nav>
  
    <% @posts.each do |post| %>
   <%= link_to post.name, id: "detail_link", remote: true %>
      <br>
     <% end %>
  </ul>
</nav>
<div id = "detail">
 
  <%= render 'detail', post: @post %>
<pagination>
   <%= paginate @posts %>
    <%= page_entries_info @posts %>
  </pagination>


</div>
</div>

<footer><center>

<%= link_to 'New Post', new_post_path %>
  
</center></footer>


